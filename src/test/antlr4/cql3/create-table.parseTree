        (statement
            (create_table_stmt
                CREATE TABLE
                (table_name timeline)
                (column_definitions
                (
                    (column_definition
                        (column_name userid)
                        (column_type (data_type (native_type uuid))))
                    ,
                    (column_definition
                        (column_name posted_month)
                        (column_type (data_type (native_type int))))
                    ,
                    (column_definition
                        (column_name posted_time)
                        (column_type (data_type (native_type uuid))))
                    ,
                    (column_definition
                        (column_name body)
                        (column_type (data_type (native_type text))))
                    ,
                    (column_definition
                        (column_name posted_by)
                        (column_type (data_type (native_type text))))
                    ,
                    (column_definition
                        PRIMARY KEY
                        (primary_key
                        (
